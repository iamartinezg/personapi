@model personapi_dotnet.Models.Entities.Telefono

@{
    ViewData["Title"] = "Eliminar Teléfono";
}

<h2>Eliminar Teléfono</h2>

<h3>¿Está seguro que desea eliminar este teléfono?</h3>
<div>
    <h4>Teléfono</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Número
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Num)
        </dd>
        <dt class="col-sm-2">
            Operador
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Oper)
        </dd>
    </dl>

    <form asp-action="DeleteConfirmed" asp-route-num="@Model.Num" method="post">
        <button type="submit" class="btn btn-danger">Eliminar</button>
        <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
    </form>
</div>